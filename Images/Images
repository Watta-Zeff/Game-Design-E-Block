import os, random, time, pygame, datetime, math

os.system('cls')

#initialize pygame

pygame.init()



WIDTH=960

HEIGHT=540

screen=pygame.display.set_mode((WIDTH,HEIGHT))

pygame.display.set_caption('Level 2')

Charizard=pygame.image.load('Images\Charizard Sprite.png')

bg=pygame.image.load('Images\Background Image lol.png')

screen.blit(bg,(0,0))

screen.blit(Charizard,(200,200))
pygame.display.update()
pygame.time.delay(3000)

